port default Payment;\r\n","C:\\Users\\richv\\OneDrive\\Documentos\\Microsoft Code Proyects\\Amazon Full Stack\\amazon-clone\\src\\axios.js",[],"C:\\Users\\richv\\OneDrive\\Documentos\\Microsoft Code Proyects\\Amazon Full Stack\\amazon-clone\\src\\Orders.js",["94"],"import React, { useEffect, useState } from \"react\";\r\nimport { db } from \"./firebase\";\r\nimport \"./Orders.css\";\r\nimport { useStateValue } from \"./StateProvider\";\r\nimport Order from \"./Order\";\r\n\r\nfunction Orders() {\r\n  const [{ basket, user }] = useStateValue();\r\n  const [orders, setOrders] = useState([]);\r\n\r\n  useEffect(() => {\r\n    if (user) {\r\n      db.collection(\"users\")\r\n        .doc(user?.uid)\r\n        .collection(\"orders\")\r\n        .orderBy(\"created\", \"desc\")\r\n        .onSnapshot((snapshot) =>\r\n          setOrders(\r\n            snapshot.docs.map((doc) => ({\r\n              id: doc.id,\r\n              data: doc.data(),\r\n            }))\r\n          )\r\n        );\r\n    } else {\r\n      setOrders([]);\r\n    }\r\n  }, [user]);\r\n\r\n  console.log(\"DEBERÍA FUNCIONAR\", Order);\r\n  return (\r\n    <div className=\"orders\">\r\n      <h1>Sus compras</h1>\r\n      <div className=\"orders__order\">\r\n        {orders?.map((order) => (\r\n          <Order order={order} />\r\n        ))}\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Orders;\r\n","C:\\Users\\richv\\OneDrive\\Documentos\\Microsoft Code Proyects\\Amazon Full Stack\\amazon-clone\\src\\Order.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":20,"column":10,"nodeType":"101","messageId":"102","endLine":20,"endColumn":12},{"ruleId":"103","severity":1,"message":"104","line":42,"column":6,"nodeType":"105","endLine":42,"endColumn":8,"suggestions":"106"},{"ruleId":"107","severity":1,"message":"108","line":10,"column":28,"nodeType":"109","messageId":"110","endLine":10,"endColumn":36},{"ruleId":"107",